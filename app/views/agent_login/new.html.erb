<header>
  <nav class="navbar sticky-top navbar-light bg-light">
    <a class="navbar-brand" href="http://localhost:3000">Real World</a>
  </nav>
</header>

<div class="agentregister scrollbar scrollbar-morpheus-den messagedagent">
  <h3>REGISTER</h3>
  <hr />
  <%= simple_form_for(@newAgent, url: agentlogin_index_path, html: { class: 'form-horizontal'}) do |f| %>
    <p>
      <%= f.label :name %> :
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :email %> :
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :password %> :
      <%= f.password_field :password %>
    </p>
    <p>
      <%= f.label :bio %> :
      <%= f.text_field :bio %>
    </p>
    <p>
      <%= f.label :specialty %> :
      <%= f.text_field :specialty %><br />
      <span>(eg. Condominium, Semi-detached House)</span>
    </p>
    <p>
      <%= f.label :location %> :
      <%= f.text_field :location %>
    </p>
    <p>
      <%= f.label :image %> :
      <%= f.text_field :img %>
    </p>
    <%= f.button :submit %>
  <% end %>
  <%= link_to 'Back', agentlogin_index_path%>
</div>
