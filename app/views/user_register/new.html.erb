<header>
  <nav class="navbar sticky-top navbar-light bg-light">
    <a class="navbar-brand" href="http://localhost:3000">Real World</a>
  </nav>
</header>

<div class="registerpage">
  <center>
    <h3>REGISTER</h3>
    <hr />
    <form class="registerform" onSubmit={this.props.getNewUser}>
      <p>name: <input placeholder="Full name" class="regname"></input></p>
      <p>email: <input placeholder="example@gmail.com" class="regemail"></input></p>
      <p>password: <input class="regpw"></input></p>
      <p>location:
        <input class="reglocation"></input>
      </p>
      <p>budget: <input class="regbudget"></input> ,000 </p>
      <p>size: <input class="regsize"></input> square ft </p>
      <p>age: <input class="regage"></input></p>
      <p>address: <input class="regadd"></input></p>
      <p>occupation: <input class="regoccupation"></input></p>
      <p><button type="submit">Register</button></p>
    </form>
    <%= link_to 'Back', root_path %>
  </center>
</div>
