<nav class="navbar sticky-top navbar-light bg-light">
  <a class="navbar-brand" href="http://localhost:3000/agent">Real World</a>
  <ul class="nav nav-pills" role="tablist">
    <li role="presentation"><a class="bannerbtn" href="http://localhost:3000/client_profile/1">Profile <span class="badge"><%= @agentmessage.length %></span></a></li>
    <li role="presentation" class="active"><a class="bannerbtn" href="http://localhost:3000/home">Log out</a></li>
  </ul>
</nav>
<div class="sorting">
  <div class="btn-group btn-group-justified active" role="group" aria-label="...">
    <div class="btn-group" role="group">
      <button type="button" class="btn btn-default">Most Reviewed</button>
    </div>
    <div class="btn-group" role="group">
      <button type="button" class="btn btn-default">Top Rated</button>
    </div>
    <div class="btn-group" role="group">
      <button type="button" class="btn btn-default">Specialty</button>
    </div>
  </div>
</div>
<div class="listofagents">
  <center><h4>A G E N T S</h4></center>
  <div class="scrollbar scrollbar-morpheus-den">
    <div class="force-overflow">
      <p>
        <%= react_component("Agentlist", props: @agent_props, prerender: false) %>
        <% @agents.each do |agent| %>
          <ul>
            <p class="listname">
              <h4><%= agent.name %> </h4>
              Specialty: <%= agent.specialty %>
              <span class="requestlink">
                <%= link_to "Request", agent %>
              </span>
            </p>
          </ul>
        <% end %>
      </p>
    </div>
  </div>
</div>
